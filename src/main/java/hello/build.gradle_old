buildscript {
  repositories {
    jcenter {
        url "https://jcenter.bintray.com/"
        }
    maven {
      url "https://plugins.gradle.org/m2/"
    }
    mavenCentral()
  }
  dependencies {
    classpath "com.cyclonedx:cyclonedx-gradle-plugin:1.2.0"
  }
}

apply plugin: "org.cyclonedx.bom"
